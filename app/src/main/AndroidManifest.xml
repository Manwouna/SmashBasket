<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.manwouna.smashbasket">

    <application
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">
        <activity android:name=".Controllers.WelcomeActvity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <meta-data
                android:name="preloaded_fonts"
                android:resource="@array/preloaded_fonts"/>

        <activity android:name=".Controllers.LeagueActivity">
        </activity>
        <activity android:name=".Controllers.BaseActivity">
        </activity>
        <activity android:name=".Controllers.SkillActivity">
        </activity>
        <activity android:name=".Controllers.FinishActivity">
        </activity>
    </application>

</manifest>